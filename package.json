    "@babel/plugin-transform-regenerator": "7.0.0",
    "@babel/runtime": "7.1.2",
    "asn1.js": "^5.0.1",
    "buffer": "^5.2.1",
    "des.js": "^1.0.0",
    "elliptic": "^6.4.1",
    "js-crypto-aes": "^0.3.0",
    "js-crypto-hash": "^0.2.0",
    "js-crypto-hmac": "^0.2.0",
    "js-crypto-random": "^0.1.2",
    "js-encoding-utils": "^0.2.7"
  }
