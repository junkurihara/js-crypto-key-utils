{
  "presets": [
    [ "@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 2 chrome versions",
          "last 2 firefox versions",
          "IE 11",
          "last 2 Edge versions"
        ]
      },
      "useBuiltIns": false
    } ],
    "babel-preset-minify"
  ],
  "ignore": [ "node_modules" ],
  "only": [ "src", "test" ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "@babel/polyfill": false,
        "regenerator": true
      }
    ]
  ]
}